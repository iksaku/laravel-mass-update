{
    "name": "iksaku/laravel-mass-update",
    "description": "Update multiple Laravel Model records, each with different values, using a single query!",
    "keywords": [
        "iksaku",
        "laravel",
        "laravel-mass-update"
    ],
    "homepage": "https://github.com/iksaku/laravel-mass-update",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Gonz√°lez",
            "email": "yo@jorgeglz.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.4",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Iksaku\\Laravel\\MassUpdate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iksaku\\Laravel\\MassUpdate\\Tests\\": "tests",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\App\\": "tests/app",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\Database\\": "tests/database",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
